                <CheckBox x:Name="checkA" Content="A" Margin="10,0,0,0" IsEnabled="False" IsChecked="True"/>
                <CheckBox x:Name="checkB" Content="B" Margin="10,0,0,0" IsChecked="True"/>
                <CheckBox x:Name="checkR1" Content="R1" Margin="10,0,0,0"/>
                <CheckBox x:Name="checkR2" Content="R2" Margin="10,0,0,0"/>
                <CheckBox x:Name="checkR3" Content="R3" Margin="10,0,0,0"/>
